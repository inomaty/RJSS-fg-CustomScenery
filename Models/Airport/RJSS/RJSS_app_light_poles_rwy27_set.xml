<PropertyList>

<!-- RJSS Custom Approach Light Set                                -->
<!-- RWY 27 Approach lights including the Flash Poles in Sendai -->
<!-- Modified from LOWI_app_light_poles_rwy26_set.xml by ot-666 -->

<nasal>
	<load>
	#loaded = 1;
		
		print("setting RJSS rwy27 sequenced flashing lights system.");
		var loop_id = 0;
		var flash_count = 0;
		
		##var info = airportinfo("RJSS");
		##var RJSS_pos = geo.Coord.new();
		##	RJSS_pos.set_latlon( info.lat, info.lon, info.elevation);
		##var aircraft_pos = geo.aircraft_position();
		##var RJSS_distance =RJSS_pos.direct_distance_to(aircraft_pos);


		
			var loop1 = func(id) {
				#id == loop_id or return;
				
				var flash_num = (flash_count += 1);
				#print ("flash_num  ", flash_num);
				
				setprop("/scenery/RJSS/RJSS_r27_app_flash/light-number", flash_num);
				
				if (flash_num == 29){
					flash_count = 0;
					#print("resetting FLASH COUNTER");
				};
				##aircraft_pos = geo.aircraft_position();
				##RJSS_distance =RJSS_pos.direct_distance_to(aircraft_pos);
				#print(RJSS_distance);
				#settimer(func { loop1(id) }, 1);
			
			
				var timer1 = maketimer(0.0167, func { loop1(id) });
				timer1.singleShot =1;
				timer1.start();
				#print("Check point1");
				var flag2 = getprop("/scenery/RJSS/RJSS_r27_app_flash/flag");
				if(flag2 == nil or flag2 == 2){
					timer1 = nil;
					}
			}
			
			
			var flag1 = getprop("/scenery/RJSS/RJSS_r27_app_flash/flag");
			if(flag1 == nil or flag1 == 2){
				loop1(loop_id);
				#print("Check point3");
				setprop("/scenery/RJSS/RJSS_r27_app_flash/flag",1);
				}
			

	</load>
	
	<unload>
		#loaded = 0;
		print("Unload RJSS rwy27 sequenced flashing lights system.");
		setprop("/scenery/RJSS/RJSS_r27_app_flash/flag",2);
	</unload>
	
</nasal>

